// Check time of 2-arg loop...
obj loop3(int n, obj res) {
  if (n == 0)
    return res;
  else
    return loop3(n - 1, res);
}
time loop3(1000000, false);

// Compare this cost to loop3:
<a> a loop2(int n, a res) {
  if (n == 0)
    return res;
  else
    return loop2<a>(n - 1, res);
}
